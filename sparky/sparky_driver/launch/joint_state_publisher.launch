<launch>
	<param name="robot_description" textfile="$(find sparky_driver)/urdf/sparky.urdf"/>
	<param name="use_gui" value="true"/>
	<param name="rate" value="30"/>
	<node pkg="joint_state_publisher" type="joint_state_publisher" name="sparky_joint_state_publisher">
		<remap from="/joint_states" to="/sparky_driver/joint_angles" />
	</node>
</launch>
